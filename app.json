{
  "expo": {
    "name": "brq_movies",
    "slug": "brq_movies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "brqmovies",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.brq-movies"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.brq_movies"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/BRQ_movies_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#16171B",
          "dark": {
            "backgroundColor": "#16171B"
          }
        }
      ],
      "expo-font",
      "react-native-edge-to-edge",
      [
        "expo-signed",
        {
          "android": {
            "storeFile": "app/keystore.jks",
            "keyAlias": "upload"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
